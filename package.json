    "@types/pg": "^8.15.5",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.16",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "yaml": "^2.8.1"
  },
  "packageManager": "pnpm@10.12.2"
